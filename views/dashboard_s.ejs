<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="./styles/dashboard_s.css" />
    <link rel="stylesheet" href="./styles/common_s.css" />

    <script src="./js/menu_s.js"></script>

    <script
      type="module"
      src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"
    ></script>
  </head>

  <body>
    <div class="outer">
      <div class="col1">
        <img src="./images/logo.jpeg" id="logo" />
        <div class="menu_items">
          <h1>MENU</h1>
          <div onclick="menu_item()" id="home" class="active">
            <ion-icon class="main-nav-icon" name="home-outline"></ion-icon>
            Home
          </div>
          <div onclick="menu_item()" id="courses">
            <ion-icon class="main-nav-icon" name="grid-outline"></ion-icon>
            Courses
          </div>

          <div onclick="menu_item()" id="grades">
            <ion-icon class="main-nav-icon" name="checkbox-outline"></ion-icon>
            Grades
          </div>

          <div onclick="menu_item()" id="tasks">
            <ion-icon class="main-nav-icon" name="easel-outline"></ion-icon>
            My Tasks
          </div>

          <div onclick="menu_item()" id="calender">
            <ion-icon class="main-nav-icon" name="calendar-outline"></ion-icon>
            Calender
          </div>

          <div onclick="menu_item()" id="inbox">
            <ion-icon class="main-nav-icon" name="albums-outline"></ion-icon>
            Inbox
          </div>

          <div onclick="menu_item()" id="settings">
            <ion-icon class="main-nav-icon" name="settings-outline"></ion-icon>
            Settings
          </div>
        </div>
      </div>
      <div class="col2">
        <h1 id="welcome">Hello</h1>
        <h2>Welcome Back</h2>
        <br /><br /><br />
        <div id="home_content">In home</div>
        <div id="courses_content">courses</div>
        <div id="grades_content">grades</div>
        <div id="tasks_content">tasks</div>
        <div id="calender_content">calender</div>
        <div id="inbox_content">inbox</div>
        <div id="settings_content">settings</div>
      </div>
      <div class="col3">
        <div style="width: 100%">
          <button id="student_logout">Logout</button>
        </div>
        <br /><br />
        <div id="dp">
          <img
            src="../images/camera.svg"
            id="profilepic"
            width="30px"
            height="30px"
          />
          <div id="blackfill"></div>
          <input type="file" id="filechooser" onchange="change()" />
        </div>
        <br /><br />
        <h2>Name</h2>
        <h3>BE-III CSE</h3>
        <br /><br />
        <h2>Notifications</h2>
        <br /><br /><br />
        <h2>Messages</h2>
      </div>
    </div>
    <p id="received_data" style="display: none"><%= data1 %></p>
    <script src="../js/dashboard_s.js"></script>
    <script>
      const home = document.getElementById("home");
      const courses = document.getElementById("courses");
      const grades = document.getElementById("grades");
      const tasks = document.getElementById("tasks");
      const calender = document.getElementById("calender");
      const inbox = document.getElementById("inbox");
      const settings = document.getElementById("settings");
      const home_content = document.getElementById("home_content");
      const courses_content = document.getElementById("courses_content");
      const grades_content = document.getElementById("grades_content");
      const tasks_content = document.getElementById("tasks_content");
      const calender_content = document.getElementById("calender_content");
      const inbox_cotnent = document.getElementById("inbox_content");
      const settings_content = document.getElementById("settings_content");
    </script>
  </body>
</html>
